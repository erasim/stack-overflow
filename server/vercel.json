{
    "version": 2,
    "builds": [
      { "src": "server/server.js", "use": "@vercel/node" },
      { "src": "client/package.json", "use": "@vercel/next" }
    ],
    "routes": [
      { "src": "^/api/(.*)", "dest": "server.js" },
      { "src": "^/(.*)", "dest": "client/pages/index.js" }
    ]
  }